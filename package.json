{
  "name": "electron-awesome-compressor",
  "version": "1.0.1",
  "description": "An Electron compressor application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "awesome-compressor",
  "homepage": "https://github.com/awesome-compressor",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:win:x64": "pnpm run build && electron-builder --win --x64",
    "build:win:arm64": "pnpm run build && electron-builder --win --arm64",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:mac:arm64": "pnpm run build && electron-builder --mac --arm64",
    "build:mac:x64": "pnpm run build && electron-builder -c electron-builder-macx64.yml --mac --x64 ",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:linux:x64": "pnpm run build && electron-builder --linux --x64",
    "build:linux:arm64": "pnpm run build && electron-builder --linux --arm64"
  },
  "dependencies": {
    "@awesome-compressor/node-image-compression": "^0.0.2",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-updater": "^6.3.9",
    "jimp": "^1.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@awesome-compressor/browser-compress-image": "^0.0.2",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/carbon": "^1.2.10",
    "@simon_he/git-fork": "^0.1.4",
    "@simon_he/git-fork-vue": "^0.0.18",
    "@sxzz/prettier-config": "^2.2.3",
    "@types/node": "^22.14.1",
    "@unocss/reset": "^0.29.6",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/core": "^8.9.4",
    "browser-image-compression": "^2.0.2",
    "bumpp": "^7.2.0",
    "compressorjs": "^1.2.1",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "element-plus": "^2.10.2",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "gifsicle-wasm-browser": "^1.5.16",
    "img-comparison-slider": "^8.0.6",
    "lazy-js-utils": "^0.1.45",
    "prettier": "^3.5.3",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.2.6",
    "vite-plugin-pages": "^0.33.1",
    "vitest": "^3.2.4",
    "vivid-typing": "^1.1.47",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "cwebp-bin",
      "electron",
      "electron-winstaller",
      "gifsicle",
      "jpegtran-bin",
      "lzo",
      "mozjpeg",
      "pngquant-bin"
    ],
    "ignoredBuiltDependencies": [
      "esbuild",
      "sharp",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
